generator client {
  provider = "prisma-client-js"
  output   = "../prismaClient/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sales {
  id          Int      @id @default(autoincrement())
  productName String
  quantity    Int
  price       Float
  totalAmount Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("sales")
}
